{
  "name": "@lightjs/utils",
  "version": "0.0.0",
  "description": "lightjs utils",
  "main": "lib",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "tsc -w",
    "test": "npm run lint && npm run type-check && npm run jest",
    "type-check": "tsc --noEmit",
    "jest": "jest --verbose --coverage",
    "lint": "eslint src . --ext js,ts",
    "ci:jest": "jest --verbose --maxWorkers=4 --coverage",
    "ci": "npm run lint && npm run ci:jest && npx codecov",
    "compile": "tsc",
    "prepublishOnly": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ludicroushq/light.git"
  },
  "keywords": [
    "lightjs",
    "utils"
  ],
  "author": "Manthan Mallikarjun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ludicroushq/light/issues"
  },
  "homepage": "https://github.com/ludicroushq/light#readme",
  "devDependencies": {
    "@lightjs/types": "0.0.0",
    "@types/jest": "^26.0.19",
    "@types/micro": "^7.3.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "find-my-way": "^3.0.5",
    "micro": "^9.3.4"
  }
}
